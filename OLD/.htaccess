# html5 pushstate (history) support:
<ifModule mod_rewrite.c>
    RewriteEngine On
	
	RewriteRule  ^([^/]+)\/(.*)$     $2 [L]
	
    RewriteCond %{REQUEST_FILENAME} !\.(js|css|gif|jpg|jpeg|png|ico|swf|pdf|html|php|json)$
    RewriteCond %{REQUEST_URI} !^index
    RewriteRule (.*) index.html [L]
	
	
</ifModule>